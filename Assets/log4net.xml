<log4net>
    <appender name="RaindropLog" type="Raindrop.RaindropAppender">
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date{HH:mm:ss} [%level] - %message" />
      </layout>
    </appender>
    <root>
      <level value="ALL" />
      <appender-ref ref="RaindropLog" />
    </root>
</log4net>
  